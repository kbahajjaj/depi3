{
  "nodes": [
    {
      "id": "1",
      "name": "Webhook Trigger",
      "type": "n8n-nodes-base.webhook",
      "parameters": {
        "httpMethod": "POST",
        "path": "submit-task",
        "responseMode": "onReceived"
      }
    },
    {
      "id": "2",
      "name": "Convert Notebook to Python",
      "type": "n8n-nodes-base.openAi",
      "parameters": {
        "operation": "chat",
        "model": "gpt-4",
        "prompt": "Convert the following Jupyter notebook JSON to a single Python script. Return only the code. Notebook content: {{$json[\"notebook\"]}}"
      }
    },
    {
      "id": "3",
      "name": "Run on Replit",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://replit.com/data/repls/<your_repl_id>/run",
        "method": "POST",
        "authentication": "headerAuth",
        "options": {"bodyContentType": "json"},
        "body": {
          "files": {"main.py": "={{$json[\"code\"]}}"},
          "command": "python3 main.py"
        },
        "headerParameters": {"Authorization": "Token <your_replit_api_token>"}
      }
    },
    {
      "id": "4",
      "name": "Run on Paperspace",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.paperspace.io/jobs",
        "method": "POST",
        "authentication": "headerAuth",
        "options": {"bodyContentType": "json"},
        "body": {
          "name": "n8n-notebook-run",
          "container": "paperspace/tensorflow-python:latest",
          "command": "papermill notebook.ipynb output.ipynb",
          "workspace": {"files": {"notebook.ipynb": "={{$json[\"notebook\"]}}"}}
        },
        "headerParameters": {
          "Authorization": "Bearer <your_paperspace_api_key>",
          "Content-Type": "application/json"
        }
      }
    },
    {
      "id": "5",
      "name": "Check Paperspace Status",
      "type": "n8n-nodes-base.httpRequest",
      "parameters": {
        "url": "https://api.paperspace.io/jobs/{{$json[\"job-id\"]}}",
        "method": "GET",
        "authentication": "headerAuth",
        "headerParameters": {"Authorization": "Bearer <your_paperspace_api_key>"}
      }
    },
    {
      "id": "6",
      "name": "Evaluate Outputs",
      "type": "n8n-nodes-base.openAi",
      "parameters": {
        "operation": "chat",
        "model": "gpt-4",
        "prompt": "Compare the following expected outputs and actual execution results. Return JSON with all_match and issues list. Expected: {{$json[\"expected_outputs\"]}}, Actual: {{$json[\"execution_results\"]}}"
      }
    },
    {
      "id": "7",
      "name": "Debug Notebook",
      "type": "n8n-nodes-base.openAi",
      "parameters": {
        "operation": "chat",
        "model": "gpt-4",
        "prompt": "Analyze this notebook code and logs to find errors and suggest fixes. Notebook: {{$json[\"notebook\"]}}, Logs: {{$json[\"logs\"]}}"
      }
    },
    {
      "id": "8",
      "name": "Generate Fixed Code",
      "type": "n8n-nodes-base.openAi",
      "parameters": {
        "operation": "chat",
        "model": "gpt-4",
        "prompt": "Provide corrected Python code based on these issues: {{$json[\"debug_report\"]}}"
      }
    },
    {
      "id": "9",
      "name": "Create Report",
      "type": "n8n-nodes-base.openAi",
      "parameters": {
        "operation": "chat",
        "model": "gpt-4",
        "prompt": "Create a professional Markdown report explaining the issues found, fixes applied, and how the new code meets requirements."
      }
    },
    {
      "id": "10",
      "name": "Email Report",
      "type": "n8n-nodes-base.emailSend",
      "parameters": {
        "fromEmail": "you@example.com",
        "toEmail": "={{$json[\"userEmail\"]}}",
        "subject": "Notebook Execution and Debug Report",
        "text": "Attached is your corrected code and analysis report.",
        "attachments": ["={{$json[\"corrected_code\"]}}", "={{$json[\"report\"]}}"]
      }
    }
  ],
  "connections": {
    "Webhook Trigger": {"main": [[{"node": "Convert Notebook to Python", "type": "main"}]]},
    "Convert Notebook to Python": {"main": [[{"node": "Run on Replit", "type": "main"}, {"node": "Run on Paperspace", "type": "main"}]]},
    "Run on Paperspace": {"main": [[{"node": "Check Paperspace Status", "type": "main"}]]},
    "Run on Replit": {"main": [[{"node": "Evaluate Outputs", "type": "main"}]]},
    "Check Paperspace Status": {"main": [[{"node": "Evaluate Outputs", "type": "main"}]]},
    "Evaluate Outputs": {"main": [[{"node": "Email Report", "type": "main"}, {"node": "Debug Notebook", "type": "main"}]]},
    "Debug Notebook": {"main": [[{"node": "Generate Fixed Code", "type": "main"}]]},
    "Generate Fixed Code": {"main": [[{"node": "Create Report", "type": "main"}]]},
    "Create Report": {"main": [[{"node": "Email Report", "type": "main"}]]}
  }
}
